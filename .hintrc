{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "disown-opener": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-box-reflect"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 122",
    "not firefox <= 122",
    "not samsung <= 23"
  ]
}